NODE_ENV=development
ENABLE_COLLECTOR=true
ENABLE_FEATURES=true
ENABLE_MODEL=true
ENABLE_SIGNAL=false

ASSETS=BTC,ETH
BAR_GRANULARITY_MIN=5
FETCH_LOOKBACK_MIN=30
CRON_COLLECT=*/1 * * * *

CRON_MODEL=*/1 * * * *
CRON_SIGNAL=*/1 * * * *

FEATURE_TTL_DAYS=14

# Aggregator
FEATURE_LAST_K=4
FEATURE_CARRY_MS=600000        # 10分钟
CRON_FEATURES=40 * * * * *

# Signals
SIGNAL_TH_UP=0.8
SIGNAL_TH_DN=-0.8
CRON_SIGNALS=45 * * * * *
DRY_RUN=1                      # 先观测，设 0 才会调用下单（已注释）

# 可选：实盘下单参数（启用时）
SIGNAL_NOTIONAL_USDT=100

OKX_BASE=https://www.okx.com
OKX_TIMEOUT_MS=20000
RETRY_5XX_TIMES=2
OKX_RETRY=3
OKX_RETRY_BACKOFF_MS=500

REDIS_URL=redis://127.0.0.1:6379/0
REDIS_TTL_DAYS=14
REDIS_NAMESPACE=quant

METRICS_PORT=9100
METRICS_PREFIX=quant_

# Mongo
MONGO_URL=mongodb://root:strong_mongo_password@localhost:27017/quant?authSource=admin

MONGO_DB=quant

# bars TTL（天）
BAR_TTL_DAYS=14

# status TTL（天）
STATUS_TTL_DAYS=7